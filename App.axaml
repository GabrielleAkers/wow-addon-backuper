<Application
    x:Class="wow_addon_backuper.App"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    RequestedThemeVariant="Default">
    <!--  "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options.  -->

    <Application.Styles>
        <FluentTheme>
            <FluentTheme.Palettes>
                <ColorPaletteResources
                    x:Key="Light"
                    Accent="#ff8961cc"
                    AltHigh="White"
                    AltLow="White"
                    AltMedium="White"
                    AltMediumHigh="White"
                    AltMediumLow="White"
                    BaseHigh="Black"
                    BaseLow="#ffeeceff"
                    BaseMedium="#ffa987bc"
                    BaseMediumHigh="#ff7b5890"
                    BaseMediumLow="#ff9270a6"
                    ChromeAltLow="#ff7b5890"
                    ChromeBlackHigh="Black"
                    ChromeBlackLow="#ffeeceff"
                    ChromeBlackMedium="#ff7b5890"
                    ChromeBlackMediumLow="#ffa987bc"
                    ChromeDisabledHigh="#ffeeceff"
                    ChromeDisabledLow="#ffa987bc"
                    ChromeGray="#ff9270a6"
                    ChromeHigh="#ffeeceff"
                    ChromeLow="#fffeeaff"
                    ChromeMedium="#fffbe4ff"
                    ChromeMediumLow="#fffeeaff"
                    ChromeWhite="White"
                    ListLow="#fffbe4ff"
                    ListMedium="#ffeeceff"
                    RegionColor="#fffef6ff" />
                <ColorPaletteResources
                    x:Key="Dark"
                    Accent="#ffffafcc"
                    AltHigh="Black"
                    AltLow="Black"
                    AltMedium="Black"
                    AltMediumHigh="Black"
                    AltMediumLow="Black"
                    BaseHigh="White"
                    BaseLow="#ff544959"
                    BaseMedium="#ffaea3b3"
                    BaseMediumHigh="#ffc5baca"
                    BaseMediumLow="#ff817686"
                    ChromeAltLow="#ffc5baca"
                    ChromeBlackHigh="Black"
                    ChromeBlackLow="#ffc5baca"
                    ChromeBlackMedium="Black"
                    ChromeBlackMediumLow="Black"
                    ChromeDisabledHigh="#ff544959"
                    ChromeDisabledLow="#ffaea3b3"
                    ChromeGray="#ff988d9d"
                    ChromeHigh="#ff988d9d"
                    ChromeLow="#ff2d1835"
                    ChromeMedium="#ff37243e"
                    ChromeMediumLow="#ff4a3d50"
                    ChromeWhite="White"
                    ListLow="#ff37243e"
                    ListMedium="#ff544959"
                    RegionColor="#ff6785a1" />
            </FluentTheme.Palettes>
        </FluentTheme>
    </Application.Styles>
</Application>
